{
  "name": "sakai-vue-ts",
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && vue-tsc --noEmit && vite build",
    "prebuild": "npm run generate:types && npm run sync:permissions && npm run validate:routes",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "type-check": "vue-tsc --noEmit",
    "update-deps": "bun scripts/update-deps.ts",
    "generate:types": "dotnet build ../NabdCare.Backend.sln && dotnet typegen generate --project-folder .. --config-path tgen.json --output-folder frontend/src/types/backend",
    "sync:permissions": "ts-node scripts/syncPermissions.ts",
    "validate:routes": "ts-node scripts/validateRoutePermissions.ts"
  },
  "dependencies": {
    "@primevue/themes": "4.4.1",
    "@tanstack/vue-query": "^5.91.1",
    "@tanstack/vue-query-devtools": "^6.0.0",
    "@vueuse/core": "^14.1.0",
    "available": "2.2.1",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "pinia": "v3.0.4",
    "primeflex": "4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "quill": "2.0.3",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.4.1",
    "@rushstack/eslint-patch": "^1.15.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "v10.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sass": "^1.94.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-primeui": "0.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "unplugin-vue-components": "30.0.0",
    "vite": "^7.2.2",
    "vue-tsc": "^3.1.3"
  },
  "trustedDependencies": [
    "sharp",
    "esbuild"
  ],
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}
